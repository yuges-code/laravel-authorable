{
    "name": "yuges-code/laravel-authorable",
    "description": "👨🏻‍🎨 Package for easily attaching authorships to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "authors",
        "laravel",
        "package",
        "authorships"
    ],
    "homepage": "https://yuges-code.github.io/laravel-authorable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-authorable",
        "issues": "https://github.com/yuges-code/laravel-authorable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "yuges-code/laravel-package": "^1.0",
        "yuges-code/laravel-orderable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Authorable\\": "src",
            "Yuges\\Authorable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Authorable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Authorable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Authorable\\Providers\\AuthorableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
